    <script src="~/Design/js/mega-menu.js"></script>
    <!-- custom jquery script -->
    <script src="~/Design/js/jquery.main.js"></script>
    <!-- REVOLUTION JS FILES -->
    <script type="text/javascript" src="~/Design/vendors/rev-slider/revolution/js/jquery.themepunch.tools.min.js"></script>
