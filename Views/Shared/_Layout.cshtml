    <link media="all" rel="stylesheet" href="~/Design/vendors/images/animate/animate.css">
    <link media="all" rel="stylesheet" href="~/Design/vendors/images/rateyo/jquery.rateyo.css">
    <link media="all" rel="stylesheet" href="~/Design/vendors/images/bootstrap-datepicker/css/bootstrap-datepicker.css">
    <link media="all" rel="stylesheet" href="~/Design/vendors/images/fancyBox/source/jquery.fancybox.css">
    <link media="all" rel="stylesheet" href="~/Design/vendors/images/fancyBox/source/helpers/jquery.fancybox-thumbs.css">

    <!-- Bootstrap 4 -->
    <link media="all" rel="stylesheet" href="~/Design/css/bootstrap.css">

    <!-- Rev Slider -->
    <link rel="stylesheet" type="text/css" href="~/Design/vendors/images/rev-slider/revolution/css/settings.css">
    <link rel="stylesheet" type="text/css" href="~/Design/vendors/images/rev-slider/revolution/css/layers.css">
    <link rel="stylesheet" type="text/css" href="~/Design/vendors/images/rev-slider/revolution/css/navigation.css">
    <!-- Theme CSS -->
    <link media="all" rel="stylesheet" href="~/Design/css/main.css">
    <!-- Custom CSS -->
    <link media="all" rel="stylesheet" href="~/Design/css/custom.css">


    <link rel="stylesheet" type="text/css" href="~/Design/w3.css" />
    <link rel="stylesheet" type="text/css" href="~/Design/css/slick.css" />
    <link rel="stylesheet" type="text/css" href="~/Design/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Design/css/custom.css">

</head>
<body class="white-overlay">



    <!-- main wrapper -->
    <div id="wrapper">
        <div id="header-holder">
            <div class="page-wrapper">
                <!-- header of the page -->
                @{ Html.RenderAction("NavHome", "Includes");}


                <!--/header of the page -->
            </div>
        </div>
    </div>


    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    @{ Html.RenderAction("LetsTalk", "Includes");}
    @{ Html.RenderAction("FooterHome", "Includes");}



    <a href="#" class="section-scroll" id="scroll-to-top"><i class="fa fa-angle-up"></i></a>
    <!-- jquery library -->
    <script src="~/Design/vendors/jquery/jquery-2.1.4.min.js"></script>
